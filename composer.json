{
  "name": "tarcisioruas/php-bitgrail-sdk",
  "license": "MIT",
  "description": "PHP BitGrail API is a PHP library for the BitGrail API designed to be easy to use.",
  "keywords": ["BitGrail", "api" , "sdk" ],
  "homepage": "https://github.com/tarcisioruas/php-bitgrail-sdk",
  "require": {
    "php" : ">=5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "psr-0": {
      "BitGrail": "src"
    },
    "psr-4": {
      "BitGrail\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "BitGrail": "tests"
    },
    "psr-4": {
      "BitGrail\\": "src/"
    }
  }
}